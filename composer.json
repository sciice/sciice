{
    "name": "sciice/sciice",
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "5.7.*",
        "spatie/laravel-permission": "^2.28",
        "qcod/laravel-imageup": "^1.0.5"
    },
    "require-dev": {
        "orchestra/testbench": "^3.7",
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Sciice\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sciice\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Sciice\\Provider\\SciiceServiceProvider"
            ],
            "aliases": {
                "Sciice": "Sciice\\Facades\\Sciice"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
